{
  "name": "casaos",
  "version": "v0.4.2",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "turbo run serve",
    "dev:noCache": "turbo run serve --no-cache",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "yarn workspaces foreach run lint"
  },
  "packageManager": "pnpm@6.35.1",
  "workspaces": [
    "main",
    "packages/*"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
